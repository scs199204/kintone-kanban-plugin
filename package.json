{
  "name": "kintone-kanban-plugin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:config": "cross-env VITE_BUILD_TARGET=config vite build",
    "build:desktop": "cross-env VITE_BUILD_TARGET=desktop vite build",
    "build": "npm run build:config && npm run build:desktop",
    "preview": "vite preview",
    "clean": "rimraf dist kintone-plugin.zip",
    "pack": "kintone-plugin-packer dist --out kintone-plugin.zip --ppk dist.njciiiepckjleombnnojicadidffmkhb.private.ppk",
    "upload": "kintone-plugin-uploader kintone-plugin.zip --base-url \"%KINTONE_BASE_URL%\" --username \"%KINTONE_USERNAME%\" --password \"%KINTONE_PASSWORD%\"",
    "build:pack": "npm run clean && npm run build && npm run pack",
    "build:upload": "npm run clean && npm run build && npm run pack && npm run upload"
  },
  "dependencies": {
    "@kintone/rest-api-client": "^5.7.5",
    "vue": "^3.5.22"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "cross-env": "^10.1.0",
    "rimraf": "^6.0.1",
    "vite": "^7.1.7",
    "vite-plugin-css-injected-by-js": "^3.5.2"
  }
}
